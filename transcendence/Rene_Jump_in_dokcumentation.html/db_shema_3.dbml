

Table Users {
  id integer [primary key]
  username varchar [not null]
  name varchar
  phone varchar
  email varchar [not null]
  ipadress varchar
  subnetmask varchar
  online bool
  avatar varchar
  statistics integer
  psw varchar [note: 'hashed']
  MFA bool [note: 'true send random email pin']
  tournomant integer
  registed timestamp
  last_login timestamp [default: `now()`]
}

Table Games_1v1_goals {
  timestamp timestamp [default: `now()`]
  interupt bool
  player1_points integer
  player2_points integer
  id integer [primary key]
}

Table Games_1v1 {
  state bool [note: '0: wait; 1: finish']
  player1_id integer
  player2_id integer
  games_id integer [primary key]
}

Table Statistics {
  id integer [primary key]
  1v1_games_loses integer
  1v1_games_wins integer 
  tournament_first integer 
  tournament_second integer
  tournament_third integer
  tournament_fourth integer
  tournament_fifth integer
  tournament_sixth integer
  tournament_sevenrh integer
  tournament_eigthth integer
}

Table Friend {
  user_id  integer 
  Frieds_id integer
}

Table Tournament_games {
  state bool [note: "0: wait; 1: finish"]
  player1_id integer
  player1_tournamentname varchar
  player2_id integer
  player2_tournamentname varchar
  player3_id integer
  player3_tournamentname varchar
  player4_id integer
  player4_tournamentname varchar
  player5_id integer
  player5_tournamentname varchar
  player6_id integer
  player6_tournamentname varchar
  player7_id integer
  player7_tournamentname varchar
  player8_id integer
  player8_tournamentname varchar
  games_id integer [primary key]
}

Table Tournament_goals {
  timestamp timestamp [default: `now()`]
  interupt bool
  player1_id integeref 
  player1_points integer
  player2_id integer
  player2_points integer
  id integer [primary key]
}


